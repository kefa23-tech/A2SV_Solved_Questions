class Solution:
    def findMinArrowShots(self, points: List[List[int]]) -> int:
        points.sort()
        ballons = len(points)

        prev = points[0]
        for i in range(1,len(points)):
            point = points[i]
            if point[0] <= prev[1]:
                ballons-=1
                prev = [point[0],min(point[1],prev[1])]
            else:
                prev = point
        return ballons
